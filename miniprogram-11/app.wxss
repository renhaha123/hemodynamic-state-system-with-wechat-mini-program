.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  flex-wrap: wrap;
  overflow: hidden;
  background-color: white;
  border: 2px solid #e2e8f0;
  border-radius: 8px;
  box-shadow: 0px 10px 25px rgba(0,0,0,0.1);
  position: relative;
  font-family: 'Droid Sans Fallback';
} 
